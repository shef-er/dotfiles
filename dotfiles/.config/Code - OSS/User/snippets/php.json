{
  "Echo": {
    "prefix": "eco",
    "body": "echo \"$0\";",
    "description": "'echo' statement"
  },

  "Include statement": {
    "prefix": "inc",
    "body": "include \"$0\";",
    "description": "'include' statement"
  },
  "Include_once statement": {
    "prefix": "inco",
    "body": "include_once \"$0\";",
    "description": "'include_once' statement"
  },

  "Require statement": {
    "prefix": "rqr",
    "body": "require \"$0\";",
    "description": "'require' statement"
  },
  "Require_once statement": {
    "prefix": "rqro",
    "body": "require_once \"$0\";",
    "description": "'require_once' statement"
  },

  "Throw new": {
    "prefix": "thr",
    "body": "throw new $0",
    "description": "throw new"
  },

  "Private method": {
    "prefix": "prif",
    "body": ["private function ${1:method}($2)$3", "{", "\t$0", "}"],
    "description": "private function"
  },
  "Private static method": {
    "prefix": "prisf",
    "body": ["private static function ${1:method}($2)$3", "{", "\t$0", "}"],
    "description": "private static function"
  },

  "Protected method": {
