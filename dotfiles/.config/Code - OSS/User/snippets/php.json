    "body": ["private function ${1:method}($2)$3", "{", "\t$0", "}"],
    "description": "private function"
  },
  "private static method": {
    "prefix": "prisf",
    "body": ["private static function ${1:method}($2)$3", "{", "\t$0", "}"],
    "description": "private static function"
