{
  "echo": {
    "prefix": "eco",
    "body": "echo \"$0\";",
    "description": "'echo' statement"
  },

  "include": {
    "prefix": "inc",
    "body": "include \"$0\";",
    "description": "'include' statement"
  },
  "include_once": {
    "prefix": "inco",
    "body": "include_once \"$0\";",
    "description": "'include_once' statement"
  },

  "require": {
    "prefix": "rqr",
    "body": "require \"$0\";",
    "description": "'require' statement"
  },
  "require_once": {
    "prefix": "rqro",
    "body": "require_once \"$0\";",
    "description": "'require_once' statement"
  },

  "throw new": {
    "prefix": "thr",
    "body": "throw new $0",
    "description": "throw new"
  },

  "private method": {
